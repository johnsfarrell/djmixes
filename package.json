{
  "name": "djmixes",
  "version": "1.0.0",
  "description": "A Web Application to allow user to upload and share DJ mixes",
  "main": "server/app.js",
  "scripts": {
    "start": "npm-run-all --parallel start-frontend start-backend",
    "start-frontend": "npm run start --prefix client",
    "start-backend": "npm run start --prefix server",
    "dev": "npm-run-all --parallel dev-frontend dev-backend",
    "dev-frontend": "npm run dev --prefix client",
    "dev-backend": "npm run dev --prefix server",
    "test": "npm-run-all --parallel test-frontend test-backend",
    "test-frontend": "npm run test --prefix client",
    "test-backend": "npm run test --prefix server",
    "build": "npm-run-all --parallel build-frontend build-backend",
    "build-frontend": "npm run build --prefix client",
    "build-backend": "npm run build --prefix server"
  },
  "author": "CS2340-Fall2024-DJMixesTeam",
  "license": "ISC",
  "dependencies": {
    "@types/aws-sdk": "^0.0.42",
    "@types/express": "^5.0.0",
    "@types/express-fileupload": "^1.5.1",
    "@types/marked": "^5.0.2",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "marked": "^14.1.4",
    "mysql2": "^3.11.4",
    "npm-run-all": "^4.1.5",
    "path": "^0.12.7",
    "request": "^2.88.2",
    "sharp": "^0.33.5"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "chai": "^4.5.0",
    "mocha": "^10.8.2",
    "nodemon": "^3.1.7",
    "supertest": "^6.3.4",
    "typescript": "^5.6.3"
  }
}
